{% extends 'base.html.twig' %}
{% block stylesheets %}
     <link href="PartenairesPage/partenairesStyle.css" rel="stylesheet" type="text/css">
{% endblock %}
{% block body %}
<div class="container-fluid text-center">
    <div class="row one m-3 ">
        <form class="row two col-sm-5 mx-auto align-items-center">
            <input type="search" id="ville" class="col-12 three form-control" placeholder="Nom de la franchise(ville)" aria-label="FranchiseName">
            <div class="row col-sm-3 four mx-auto align-items-center">
                <div class="form-check">
                    <label class="form-check-label" for="flexCheckChecked">Actif</label>
                    <input class="form-check-input" type="checkbox" value="" id="flexCheckChecked" checked>                                             
                </div>
                <div class="form-check">
                    <label class="form-check-label" for="flexCheckDefault">Non actif</label>
                    <input class="form-check-input" type="checkbox" value="" id="flexCheckDefault">                        
                </div>
            </div> 
        </form>
    </div> 
    <div id="result" class="row five"></div>
    
    {% block javascripts %}
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.1/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-OERcA2EqjJCMA+/3y+gxIOqMEjwtxJY7qPCqsdltbNJuaOe923+mo//f6V8Qbsw3" crossorigin="anonymous"></script>
    <script src="SallesDesPartenairesPage/sallesDesPartenaires.js"></script>
    {% endblock %}
</div>
{% endblock %}